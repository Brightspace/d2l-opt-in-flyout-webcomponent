{
  "description": "A webcomponent for an opt-in/opt-out flyout widget",
  "keywords": [
    "d2l",
    "opt-in",
    "opt-out"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/d2l-opt-in-flyout-webcomponent.git"
  },
  "name": "d2l-opt-in-flyout-webcomponent",
  "version": "2.0.0",
  "scripts": {
    "start": "polymer serve",
    "test": "npm run test:lint && npm run test:polymer:local",
    "test:lint": "npm run test:lint:wc && npm run test:lint:js",
    "test:lint:js": "eslint . --ext .js,.html test/**/*.js test/**/*.html demo/**/*.js demo/**/*.html",
    "test:lint:wc": "polymer lint",
    "test:polymer:local": "polymer test --skip-plugin sauce",
    "test:polymer:sauce": "polymer test --skip-plugin local"
  },
  "author": "D2l Corporation",
  "devDependencies": {
    "@polymer/iron-test-helpers": "^3.0.0",
    "@webcomponents/webcomponentsjs": "^2.2.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^4.19.1",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.5",
    "polymer-cli": "^1.9.3",
    "wct-browser-legacy": "^1.0.1"
  },
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "dependencies": {
    "@polymer/polymer": "^3.0.0",
    "d2l-colors": "BrightspaceUI/colors#semver:^4",
    "d2l-icons": "BrightspaceUI/icons#semver:^6",
    "d2l-inputs": "BrightspaceUI/inputs#semver:^2",
    "d2l-offscreen": "BrightspaceUI/offscreen#semver:^4",
    "d2l-button": "BrightspaceUI/button#semver:^5",
    "d2l-typography": "BrightspaceUI/typography#semver:^7"
  }
}
